<ul class="pagination {{ cssMix }}">
  {% for item in list %}
    <li class="pagination-item">
      <button
        class="pagination-link {{ 'pagination-link-current' if item.current }}"
        name="page"
        type="submit"
        value="{{ item.value }}"
        {{ "disabled" if item.current }}
      >
        {{ item.title }}
      </button>
    </li>
  {% endfor %}
</ul>
